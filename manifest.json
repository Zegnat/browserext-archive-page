{
  "name": "Archive Page",
  "author": "John Navas",
  "manifest_version": 2,
  "version": "1.2.1",
  "description": "Archive webpage with Archive Today",
  "homepage_url": "https://github.com/JNavas2/Archive-Page",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "optional_permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon-16.png",
      "24": "images/icon-24.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "64": "images/icon-64.png",
      "96": "images/icon-96.png",
      "128": "images/icon-128.png"
    },
    "default_title": "Share URL to Archive Page"
  },
  "icons": {
    "16": "images/icon-16.png",
    "24": "images/icon-24.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "commands": {
    "myArchive": {
      "suggested_key": {
        "default": "Ctrl+Alt+A"
      },
      "description": "Share URL to Archive Page"
    },
    "mySearch": {
      "suggested_key": {
        "default": "Ctrl+Alt+F"
      },
      "description": "Search (Find) Archive Today for current page URL"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{5b22cb75-8e43-4f2a-bb9b-1da0655ae564}",
      "strict_min_version": "113.0"
    }
  }
}